<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef>
    <defName>chemicalMake</defName>
    <label>create compound</label>
    <description>Create a chemical compound from stuff you don't need.</description>
    <jobString>Producing chemicals.</jobString>
    <workAmount>800</workAmount>
    <workSkill>Crafting</workSkill>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <workSkillLearnFactor>0.2</workSkillLearnFactor>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Root</li>
          </categories>
          <disallowedThingDefs>
            <li>Chemicals</li>
          </disallowedThingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Root</li>
      </categories>
      <disallowedThingDefs>
        <li>Chemicals</li>
      </disallowedThingDefs>
    </fixedIngredientFilter>
    <products>
      <Chemicals>1</Chemicals>
    </products>
  </RecipeDef>
