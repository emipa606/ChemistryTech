<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef>
    <defName>ionicMake</defName>
    <label>create ionic mass</label>
    <description>Create a ionic particle mass from chemicals.</description>
    <jobString>Producing particle mass.</jobString>
    <workAmount>2000</workAmount>
    <workSkill>Crafting</workSkill>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <workSkillLearnFactor>0.3</workSkillLearnFactor>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Chemicals</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Chemicals</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Ionic>1</Ionic>
    </products>
  </RecipeDef>
