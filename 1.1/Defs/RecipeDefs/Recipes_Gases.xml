<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef>
    <defName>chemicalMakeAnesthezineGas</defName>
    <label>create anesthezine</label>
    <description>Fill gas tank with anesthezine gas.</description>
    <jobString>Filling tank.</jobString>
    <workAmount>500</workAmount>
    <workSkill>Crafting</workSkill>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <workSkillLearnFactor>0.2</workSkillLearnFactor>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RefinedChemicals</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RefinedChemicals</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <GasAnesthezine>5</GasAnesthezine>
    </products>
    <researchPrerequisite>ChemistryGases</researchPrerequisite>
  </RecipeDef>
